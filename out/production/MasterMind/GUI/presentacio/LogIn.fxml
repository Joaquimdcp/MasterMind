<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-image: url(/GUI/assets/bg.jpg)" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.presentacio.LogInController">
  <children>
    <VBox alignment="CENTER" layoutY="58.0" prefHeight="542.0" prefWidth="800.0" spacing="50.0">
      <children>
        <TextField fx:id="usernameText" prefWidth="200.0" promptText="Username">
          <VBox.margin>
            <Insets left="75.0" right="75.0" fx:id="x1" />
          </VBox.margin>
        </TextField>
        <PasswordField fx:id="passwordText" prefWidth="200.0" promptText="Password" text="">
          <VBox.margin>
            <Insets left="75.0" right="75.0" fx:id="x2" />
          </VBox.margin>
        </PasswordField>
        <Button mnemonicParsing="false" onAction="#loginPressed" prefHeight="70.0" prefWidth="210.0" style="-fx-background-image: url(/GUI/assets/submit.png); -fx-border-color: transparent; -fx-border-width: 0; -fx-background-radius: 0; -fx-background-color: transparent;" />
        <ImageView fx:id="errorImage" fitHeight="0" fitWidth="0" pickOnBounds="true" preserveRatio="true" />
        <HBox>
           <children>
             <Button mnemonicParsing="false" prefHeight="69.0" prefWidth="100.0" style="-fx-border-color: transparent; -fx-border-width: 0; -fx-background-radius: 0; -fx-background-color: transparent;" />
             <ImageView pickOnBounds="true" preserveRatio="true">
               <image>
                 <Image url="@../assets/registertext.png" />
               </image>
                     <HBox.margin>
                        <Insets top="17.0" />
                     </HBox.margin>
             </ImageView>
             <Button mnemonicParsing="false" onAction="#registerPressed" prefHeight="69.0" prefWidth="210.0" style="-fx-background-image: url(/GUI/assets/register.png); -fx-border-color: transparent; -fx-border-width: 0; -fx-background-radius: 0; -fx-background-color: transparent;" />
           </children>
        </HBox>
        </children>
      <padding>
        <Insets />
      </padding>
    </VBox>
    <ImageView layoutX="305.0" layoutY="30.0" pickOnBounds="true" preserveRatio="true">
      <image>
        <Image url="@../assets/logintitle.png" />
      </image>
    </ImageView>
  </children>
</AnchorPane>
